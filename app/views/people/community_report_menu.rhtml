<form name='community_report' method='post' action="community_vitals_report" >
  <% month_names = month_name_options.gsub('<option value="Unknown">Unknown</option>', '') %>
 

	<%= text_field_tag "start_year", nil,
          {:helpText => 'Chaka Choyambira',
          :field_type => 'number',
          :absoluteMin => "1890",
          :min => "1940",
          :absoluteMax => Date.today.year,
          :tt_pageStyleClass => "Numeric NumbersOnly",
          :tt_onLoad => "changeKeyboardButtonText()"
         }  %>
  
	<%= select_tag "start_month", month_names,
          {:helpText => 'Mwenzi Woyambira',
          :validationMessage => 'Please enter a valid date',
          :tt_onLoad => "changeKeyboardButtonText()"}%>

	<%= text_field_tag "start_day",  nil, {:field_type => 'number',
          :helpText => "Tsiku Loyambira" ,
          :validationJS => "validateDOB()",
          :tt_pageStyleClass => "Numeric NumbersOnly",
          :validationMessage => 'Please enter a valid date',
          :tt_onLoad => "changeKeyboardButtonText()"}%>


        <%= text_field_tag "end_year", nil,
          {:helpText => 'Chaka Chomalizira',
          :field_type => 'number',
          :absoluteMin => "1890",
          :min => "1940",
          :absoluteMax => Date.today.year,
          :tt_pageStyleClass => "Numeric NumbersOnly",
          :tt_onLoad => "changeKeyboardButtonText()"
         }  %>

	<%= select_tag "end_month", month_names,
          {:helpText => 'Mwezi Womalizira',
          :validationMessage => 'Please enter a valid date',
          :tt_onLoad => "changeKeyboardButtonText()"}%>

	<%= text_field_tag "end_day",  nil, {:field_type => 'number',
          :helpText => "Tsiku Lomalizira" ,
          :validationJS => "validateDOB()",
          :tt_pageStyleClass => "Numeric NumbersOnly",
          :validationMessage => 'Please enter a valid date',
          :tt_onLoad => "changeKeyboardButtonText()"}%>
	
</form>
