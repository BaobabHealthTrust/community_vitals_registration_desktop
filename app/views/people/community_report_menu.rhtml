  }
  #tt_page_mwenzi_woyambira .inputFrameClass{
    height:563px !important;
  }
   #tt_page_mwenzi_woyambira .options{
    height:515px !important;
  }

  .tt_controls_mwezi_womalizira #keyboard{
    display:none !important;
  }
  #tt_page_mwezi_womalizira .inputFrameClass{
